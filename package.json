{
  "name": "react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "lint:stylelint:fix": "npm run lint:stylelint -- --fix",
    "lint": "npm-run-all --parallel 'lint:*[!fix]'",
    "lint:fix": "npm-run-all --parallel 'lint:*:fix'",
    "format": "prettier --write .",
    "preview": "vite preview",
    "scan": "depcheck && ts-prune && unimported",
    "prepare": "husky",
    "commit": "cz"
  },
  "dependencies": {
    "face-api.js": "^0.22.2",
    "handtrackjs": "^0.1.5",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "react-toastify": "^10.0.5",
    "react-webcam": "^7.2.0",
    "three": "^0.167.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "stylelint": "^16.8.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2",
    "unimported": "^1.31.1",
    "vite": "^5.3.4",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-pretty-module-classnames": "^1.2.0",
    "vite-plugin-sass-dts": "^1.3.25",
    "vite-plugin-stylelint": "^5.3.1"
  },
  "packageManager": "yarn@4.0.2+sha512.4e502bea682e7d8004561f916f1da2dfbe6f718024f6aa50bf8cd86f38ea3a94a7f1bf854a9ca666dd8eafcfb8d44baaa91bf5c7876e79a7aeac952c332f0e88",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
